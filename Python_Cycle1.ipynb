      "source": [
        "Q18. Program to Reverse a Number"
      ]
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "Coy6Y_5hA8oq",
        "outputId": "e011ab1e-ebf5-4085-eac3-a3771dd12bbe",
        "colab": {
          "base_uri": "https://localhost:8080/"
        }
      },
      "source": [
        "n=int(input(\"Enter number: \"))\r\n",
        "rev=0\r\n",
        "while(n>0):\r\n",
        "    dig=n%10\r\n",
        "    rev=rev*10+dig\r\n",
        "    n=n//10\r\n",
        "print(\"Reverse of the number:\",rev)"
      ],
      "execution_count": 37,
      "outputs": [
        {
          "output_type": "stream",
          "text": [
            "Enter number: 345\n",
            "Reverse of the number: 543\n"
          ],
          "name": "stdout"
        }
      ]
